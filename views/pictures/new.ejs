<% include ../partials/header %>

<div class="container boxer">
    <h3 class="text-center">Add new pictures to <%= blog.title %></h3>
    <form action="/blogs/<%= blog._id %>/pictures" method="POST">
        <div class="form-group">
            <label>Image</label>
            <input class="form-control" type="text" name="picture[image]" placeholder="Image URL">
        </div>
        <button type="submit" class="btn btn-primary centered">Submit</button>
    </form>
</div>





<% include ../partials/footer %>